class Solution {
public:
    bool searchMatrix(vector<vector<int>>& matrix, int target) {
        int row = matrix.size();
        int col = matrix[0].size();
        int i = row-1;
        int j = 0;
      
            for(; j<col && i>=0; ){
                if(matrix[i][j]==target)return true;
                else if(matrix[i][j]>target) i--;
                else j++;
            }
        
        return false;
    }
};
